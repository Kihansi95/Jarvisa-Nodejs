<div class="container">
	<div class="page-header" style="padding: 0; margin: 0;">
		<h1 style="margin-top: 0;">
			Data
			<small>In case you missed something</small>
		</h1>
	</div>

	<section ng-controller="DataCtrl">
		<div ng-repeat="data in dataEntries | orderBy:'+':true" class="data-entry">
			<span class="pull-right text-muted" am-time-ago="data.createdAt"></span>
			<i class="fa fa-{{data.icon}} text-muted"></i>
			{{data.device}}
			<small class="meta text-muted">
				{{data.content}}
			</small>
		</div>
	</section>
</div>
